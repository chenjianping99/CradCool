package com.jiubang.goscreenlock.theme.cjpcardcool.weather.location.celllocation;

import android.location.Location;

/**
 * 
 * 类描述: 定位回调接口
 * 
 * @author  liuwenqin
 * @date  [2012-9-4]
 */
public interface CellLocationListener
{
	/**基站定位成功。
	 * @param location 当前位置的经纬度*/
	public void onLocationComplete(Location location);
	/**基站定位失败*/
	public void onLocationFailed();
}