package com.jiubang.goscreenlock.theme.cjpcardcool.calendar;

import java.util.List;

/**
 * 日程消息
 * 
 * @author zhangjie
 * 
 */
public class ScheduleInfo
{
	private String		mTitle;			// 日程名
	private String		mPlace;			// 地点
	private long		mEventBegin;
	private long		mEventEnd;
	private long		mRemindersTime;
	private boolean		mHasAlarm;
	private long		mEventId;
	private List<Long>	mRemindersTimeList;
	private String 		mEventBeginStr;
	private String 		mEventEndStr;
	
	public String getmEventBeginStr()
	{
		return mEventBeginStr;
	}

	public void setmEventBeginStr(String mEventBeginStr)
	{
		this.mEventBeginStr = mEventBeginStr;
	}

	public String getmEventEndStr()
	{
		return mEventEndStr;
	}

	public void setmEventEndStr(String mEventEndStr)
	{
		this.mEventEndStr = mEventEndStr;
	}

	public List<Long> getmRemindersTimeList()
	{
		return mRemindersTimeList;
	}

	public void setmRemindersTimeList(List<Long> mRemindersTimeList)
	{
		this.mRemindersTimeList = mRemindersTimeList;
	}

	public long getmRemindersTime()
	{
		return mRemindersTime;
	}

	public void setmRemindersTime(long mRemindersTime)
	{
		this.mRemindersTime = mRemindersTime;
	}

	public long getmEventId()
	{
		return mEventId;
	}

	public void setmEventId(long mEventId)
	{
		this.mEventId = mEventId;
	}

	public boolean ismHasAlarm()
	{
		return mHasAlarm;
	}

	public void setmHasAlarm(boolean mHasAlarm)
	{
		this.mHasAlarm = mHasAlarm;
	}

	public String getmPlace()
	{
		return mPlace;
	}

	public void setmPlace(String mPlace)
	{
		this.mPlace = mPlace;
	}

	public long getmEventBegin()
	{
		return mEventBegin;
	}

	public void setmEventBegin(long mEventBegin)
	{
		this.mEventBegin = mEventBegin;
	}

	public long getmEventEnd()
	{
		return mEventEnd;
	}

	public void setmEventEnd(long mEventEnd)
	{
		this.mEventEnd = mEventEnd;
	}

	public String getmTitle()
	{
		return mTitle;
	}

	public void setmTitle(String mTitle)
	{
		this.mTitle = mTitle;
	}
}